{
    "name": "wovoschool/laravel-language",
    "description": "Language switcher package for Laravel",
    "keywords": [
        "laravel",
        "language",
        "switcher"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Bishwajit Adhikary",
            "email": "bishwajitcadhikary@gmail.com",
            "homepage": "https://wovoschool.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": ">=5.4.0",
        "jenssegers/agent": "2.6.*"
    },
    "autoload": {
        "psr-4": {
            "WovoSchool\\Language\\": "./src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "WovoSchool\\Language\\LanguageServiceProvider"
            ],
            "aliases": {
                "Language": "WovoSchool\\Language\\Facade"
            }
        }
    }
}
